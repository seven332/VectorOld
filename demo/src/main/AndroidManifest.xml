<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hippo.vectorold.demo">

    <application
        android:name="com.hippo.vectorold.demo.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".vector.VectorActivity"
            android:label="@string/activity_vector">
        </activity>

        <activity
            android:name=".progress.ProgressActivity"
            android:label="@string/activity_progress">
        </activity>

    </application>

</manifest>
